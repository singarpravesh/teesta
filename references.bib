keywords = {Ecology,Freshwater & Marine Ecology,Zoology},
month = {jan},
number = {1},
pages = {3--19},
publisher = {Springer},
title = {{Evaluating good-practice cases for river restoration across Europe: context, methodological framework, selected results and recommendations}},
url = {https://link.springer.com/article/10.1007/s10750-016-2652-7},
