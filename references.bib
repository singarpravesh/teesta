journal = {Ecological Economics},
keywords = {China,Contingent Valuation Method,Total economic value,Willingness to pay},
number = {2-3},
pages = {345--358},
title = {{Applying contingent valuation in China to measure the total economic value of restoring ecosystem services in Ejina region}},
volume = {44},
year = {2003}
